pipeline {
    agent any
    stages {
       stage('get python and modules'){
            steps {
                sh 'apt update'
                sh 'apt install python3.10 -y'
                sh 'apt install python3-pip -y'
                sh 'pip install -r requirements.txt'
                   }
        }
       stage('runZtool'){
            steps {
                sh 'python3 main.py'
                   }
        }
    }
    }